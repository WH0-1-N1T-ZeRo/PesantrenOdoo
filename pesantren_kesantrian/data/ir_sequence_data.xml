<?xml version='1.0' encoding='utf-8'?>
<odoo>
  <data noupdate="1">

  <!-- sequence kesehatan -->
    <record id="sequence_kesehatan" model="ir.sequence">
        <field name="name">kesehatan</field>
        <field name="code">cdn.kesehatan</field>
        <field name="prefix">KS/%(y)s.%(month)s/</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="implementation">standard</field>
    </record>  
  
    <!-- ir.sequence for cdn.pelanggaran -->
    <record id="cdn_pelanggaran_sequence" model="ir.sequence">
        <field name="name">No Referensi Pelanggaran</field>
        <field name="code">cdn.pelanggaran</field>
        <field name="prefix">KP/%(y)s.%(month)s/</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="implementation">standard</field>
        <field name="active">True</field>
    </record>

    <!-- ir.sequence for cdn.perijinan -->
    <record id="cdn_perijinan_sequence" model="ir.sequence">
        <field name="name">No Referensi Perijinan</field>
